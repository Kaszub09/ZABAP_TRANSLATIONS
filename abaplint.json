        {
          "pseudo": "#EC CI_SUBRC",
          "pragma": "SUBRC_OK"
        },
        {
          "pseudo": "#EC NEEDED",
          "pragma": "NEEDED"
        },
        {
          "pseudo": "#EC NOTEXT",
          "pragma": "NO_TEXT"
        },
        {
          "pseudo": "#EC NO_HANDLER",
