    "preferred_compare_operator": {
      "exclude": [],
      "severity": "Error",
      "badOperators": [
        "EQ",
        "><",
        "NE",
        "GE",
        "GT",
        "LT",
        "LE"
      ]
    }, 
    "select_performance": {
      "exclude": [],
      "severity": "Warning",
      "endSelect": true,
      "selectStar": false,
